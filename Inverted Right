#include<stdio.h>
int main()
{
    int rows, i, j, s;
    printf("Enter the  number of rows: ");
    scanf("%d",&rows);
    for(i=rows;i>=1;i--)
    {
        for(s=0;s<rows-i;s++)
        {
            printf(" ");
        }
        for(j=0;j<i;j++)
        {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
