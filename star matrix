#include<stdio.h>
int main()
{
    int row,col,i,j;
    printf("enter number of columns:");
    scanf("%d",&col);
    printf("enter number of rows:");
    scanf("%d",&row);
    for(i=0;i<row;i++)
    {
     for(j=0;j<col;j++)  
    {
        printf("* ");
    }
    printf("\n");
    }   
    return 0;
}
